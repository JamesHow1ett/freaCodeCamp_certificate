{
  "extends": ["react-app", "react-app/jest", "airbnb", "plugin:jsx-a11y/recommended", "prettier"],
  "plugins": ["jsx-a11y", "prettier"],
  "rules": {
    "import/prefer-default-export": "off",
    "no-param-reassign": "off",
    "no-unused-vars": "warn",
    "semi": "warn",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "prettier/prettier": [
      "warn",
      {
        "semi": true
      }
    ]
  }
}
